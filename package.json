{
  "name": "react-loader",
  "version": "1.0.0",
  "description": "A lightweight and easy-to-use loading bar written in React JS",
  "main": "dist/react-loader.js",
  "scripts": {
    "deploy": "npm run build",
    "start": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --config webpack.production.config.js",
    "test": "mocha --compilers js:babel-register",
    "babel-build": "babel src -d dist",
    "prepublish": "npm run build",
    "lint": "eslint src/**",
    "gulp-lint": "gulp lint",
    "lint:watch": "esw -w src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teekenl/react-loader.git"
  },
  "keywords": [
    "React",
    "Loading",
    "react-component",
    "spin loader",
    "react-loading",
    "widget"
  ],
  "author": "teekenl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teekenl/react-loader/issues"
  },
  "homepage": "https://github.com/teekenl/react-loader#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "style-loader": "^0.13.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "mocha": "^3.5.3",
    "uglify-js": "^3.1.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  }
